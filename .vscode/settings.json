{
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
      "javascript",
      { "language": "typescript", "autoFix": true }
    ],
    "editor.formatOnSave": true,
    "[javascript]": {
      "editor.formatOnSave": false
    },
    "[typescript]": {
      "editor.formatOnSave": false
    },
    "files.autoSave": "onFocusChange",
    "git.rebaseWhenSync": true,
    "files.exclude": {
      "**/node_modules": true
    },
    "search.exclude": {
      "**/dist": true
    }
  }
  